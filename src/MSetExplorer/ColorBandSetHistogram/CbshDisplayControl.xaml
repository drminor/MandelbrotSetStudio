<UserControl x:Class="MSetExplorer.CbshDisplayControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MSetExplorer"
             mc:Ignorable="d" d:DesignWidth="1044" d:DesignHeight="240">
    <Grid>
        <Border x:Name="TopBorder"  BorderThickness="0" BorderBrush="Black" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="3">
            <Canvas Name="MainCanvas"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="Transparent"/>
        </Border>

    </Grid>
</UserControl>
