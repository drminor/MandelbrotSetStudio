<Window x:Class="MSetExplorer.AppNavWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MSetExplorer"
        mc:Ignorable="d"
        Title="AppNavWindow" Height="150" Width="200">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button Width="100" Height="25" Click="GoButton_Click" Margin="0,10">Go</Button>
            <Button Width="100" Height="25" Click="LeaveButton_Click" >Leave</Button>

        </StackPanel>
    </Grid>
</Window>
