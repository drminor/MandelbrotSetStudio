<Window x:Class="MSetExplorer.AppNavWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MSetExplorer"
        mc:Ignorable="d"
        Title="AppNavWindow" Height="325" Width="400">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button Width="100" Height="25" Click="ExploreButton_Click" Margin="0,10">Explore</Button>
            
            <Button Width="100" Height="25" Click="DesignPosterButton_Click" Margin="0,10">Design Poster</Button>
            
            <Button Width="100" Height="25" Click="LeaveButton_Click" Margin="0,10" >Leave</Button>

            <Button Width="200" Click="RemoveMapSectionsButton_Click" Margin="0,120">Remove Recent MapSections</Button>

        </StackPanel>
    </Grid>
</Window>
