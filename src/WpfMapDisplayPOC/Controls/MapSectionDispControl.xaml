<UserControl x:Class="WpfMapDisplayPOC.MapSectionDispControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfMapDisplayPOC"
             mc:Ignorable="d" 
             d:DesignHeight="1100" d:DesignWidth="1100"
             x:Name="bmGrid">
    <Grid>
           
        <Canvas Name="myCanvas">
            <Image Name="myImage" Source="{Binding ElementName=bmGrid, Path=Bitmap}"/>
        </Canvas>

        <!--<Canvas Name="myCanvas">
            <Image Name="myImage" Source="{Binding ElementName=bmGrid, Path=Bitmap}"/>
        </Canvas>-->


    </Grid>
</UserControl>

